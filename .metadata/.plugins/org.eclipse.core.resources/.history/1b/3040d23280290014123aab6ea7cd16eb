import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

import Utilities.IO_Template.MyScanner;

/**
 * Codeforces.com 
 * Round 261 
 * Division 2
 * Problem A
 * @author Edward McEnrue
 */
public class CF261_Pashmak_and_Garden {

	public static void main(String[] args) {
		MyScanner in = new MyScanner();

		int x1 = in.nextInt();
		int y1 = in.nextInt();
		int x2 = in.nextInt();
		int y2 = in.nextInt();
		
		int x3 = 0, y3 = 0, x4 = 0, y4 = 0;
		
		boolean works = false;
		if(x1 == y1 && x2 == y2) // diag at origin
		{
			x3 = Math.max(x1, x2);
			y3 = 0;
			
			x4 = 0;
			y4 = Math.max(y1, y2);
			works = true;
		}
		else if(x1 != y1 && x2 != y2 && x1 == y2 && y1 == x2 && (x1 == 0 || y1 == 0) && (x2 == 0 || y2 == 0)) // diag not origin
		{
			//will need to constrain it further to say x1 == y2 && y1 == x2 
			x3 = 0;
			y3 = 0;
			
			x4 = x1;
			y4 = y1;
		}
		else if(x1 == x2 && (y1 == 0 || y2 == 0)) // column
		{
			if(x1 == 0)
			{
				x3 = Math.max(y1, y2);
				y3 = Math.max(y1, y2);
				
				x4 = Math.max(y1, y2);
				y4 = 0;
			}
			else
			{
				x3 = 0;
				y3 = Math.max(y1, y2);
				
				x4 = 0;
				y4 = 0;
			}
		}
		else if(y1 == y2 && (x1 == 0 || x2 == 0)) // row
		
		if(works)
		{
			System.out.println(x3 + " " + y3 + " " + x4 + " " + y4);
		}
		else
		{
			System.out.println("-1");
		}
		
	}

	// -----------MyScanner class for faster input----------
	public static class MyScanner {
		BufferedReader br;
		StringTokenizer st;

		public MyScanner() {
			br = new BufferedReader(new InputStreamReader(System.in));
		}

		String next() {
			while (st == null || !st.hasMoreElements()) {
				try {
					st = new StringTokenizer(br.readLine());
				} catch (IOException e) {
					e.printStackTrace();
				}
			}
			return st.nextToken();
		}

		int nextInt() {
			return Integer.parseInt(next());
		}

		long nextLong() {
			return Long.parseLong(next());
		}

		double nextDouble() {
			return Double.parseDouble(next());
		}

		String nextLine() {
			String str = "";
			try {
				str = br.readLine();
			} catch (IOException e) {
				e.printStackTrace();
			}
			return str;
		}

	}
	// --------------------------------------------------------
}
